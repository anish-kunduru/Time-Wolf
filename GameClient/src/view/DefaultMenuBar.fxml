<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DefaultMenuBarController">
	<children>
				<MenuBar fx:id="menuBar" prefWidth="1280.0" HBox.hgrow="ALWAYS">
					<menus>
						<Menu mnemonicParsing="false" text="File">
							<items>
								<MenuItem fx:id="fileNewGame" mnemonicParsing="false" onAction="#initialize" text="New Game" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Edit">
							<items>
								<MenuItem fx:id="editProfile" mnemonicParsing="false" onAction="#initialize" text="Profile" />
							</items>
						</Menu>
						<Menu mnemonicParsing="false" text="Help">
							<items>
								<MenuItem fx:id="helpAbout" mnemonicParsing="false" onAction="#initialize" text="About" />
							</items>
						</Menu>
					</menus>
				</MenuBar>
	</children>
</HBox>
