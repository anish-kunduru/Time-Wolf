<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="userListing.UserListingScreenController">
	<children>
		<fx:include source="../menuBar/DefaultMenuBar.fxml" />
		<TableView fx:id="userTable" prefHeight="695.0" prefWidth="400.0"
			AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="25.0">
			<columns>
				<TableColumn fx:id="usernameColumn" editable="false"
					prefWidth="139.0" text="Username" />
				<TableColumn fx:id="emailColumn" editable="false"
					prefWidth="122.0" text="E-mail" />
				<TableColumn fx:id="bannedColumn" editable="false"
					prefWidth="66.0" text="Banned?" />
				<TableColumn fx:id="roleColumn" editable="false"
					prefWidth="72.0" text="Role" />
			</columns>
		</TableView>
		<Button fx:id="saveChangesButton" layoutX="1085.0" layoutY="651.0"
			mnemonicParsing="false" text="Save Changes" AnchorPane.bottomAnchor="44.0"
			AnchorPane.rightAnchor="105.0" />
		<Button fx:id="cancelButton" layoutX="1197.0" layoutY="651.0"
			mnemonicParsing="false" text="Cancel" AnchorPane.bottomAnchor="44.0"
			AnchorPane.rightAnchor="31.0" />
		<Label fx:id="errorLabel" alignment="CENTER" layoutX="437.0"
			layoutY="580.0" prefHeight="25.0" prefWidth="630.0" textFill="RED">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<ImageView fx:id="profilePictureImageView" fitHeight="250.0"
			fitWidth="250.0" layoutX="1011.0" layoutY="46.0" pickOnBounds="true"
			preserveRatio="true" AnchorPane.rightAnchor="20.0"
			AnchorPane.topAnchor="45.0" />
		<Button fx:id="removePhotoButton" layoutX="1167.0" layoutY="309.0"
			mnemonicParsing="false" text="Remove Photo" />
		<GridPane layoutX="458.0" layoutY="67.0" prefHeight="308.0"
			prefWidth="477.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0"
					minWidth="10.0" prefWidth="138.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="339.0"
					minWidth="10.0" prefWidth="339.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Text layoutX="465.0" layoutY="87.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Username:">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<Text layoutX="458.0" layoutY="219.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Reason:" GridPane.rowIndex="3">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextField fx:id="emailTextField" layoutX="603.0" layoutY="96.0"
					GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<TextField fx:id="usernameTextField" layoutX="603.0"
					layoutY="67.0" GridPane.columnIndex="1" />
				<Text layoutX="458.0" layoutY="128.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="E-mail:" GridPane.rowIndex="1">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<TextArea fx:id="bannedReasonTextArea" layoutX="578.0"
					layoutY="113.0" prefHeight="200.0" prefWidth="200.0"
					GridPane.columnIndex="1" GridPane.rowIndex="3" />
				<Text layoutX="458.0" layoutY="177.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Banned:" GridPane.rowIndex="2">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<Text layoutX="458.0" layoutY="266.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Role:" GridPane.rowIndex="4">
					<font>
						<Font size="18.0" />
					</font>
				</Text>
				<CheckBox fx:id="bannedCheckBox" mnemonicParsing="false"
					text="No" GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<HBox alignment="CENTER" spacing="45.0" GridPane.columnIndex="1"
					GridPane.rowIndex="4">
					<children>
						<CheckBox fx:id="administratorRoleCheckBox"
							mnemonicParsing="false" text="Administrator"
							GridPane.columnIndex="1" GridPane.rowIndex="4" />
						<CheckBox fx:id="userRoleCheckBox" mnemonicParsing="false"
							text="User" GridPane.columnIndex="1" GridPane.rowIndex="4" />
						<CheckBox fx:id="moderatorRoleCheckBox" mnemonicParsing="false"
							text="Moderator" GridPane.columnIndex="1" GridPane.rowIndex="4" />
					</children>
					<padding>
						<Insets left="5.0" />
					</padding>
				</HBox>
			</children>
		</GridPane>
		<Button fx:id="resetPasswordButton" layoutX="458.0" layoutY="395.0"
			mnemonicParsing="false" text="Reset Password" />
	</children>
</AnchorPane>
